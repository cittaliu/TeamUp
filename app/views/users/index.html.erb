<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <h5>Team Creator</h5>
    <div class="col-xs-6 col-md-3 objectives">
        <%= link_to user_path(@creator) do %>
        <img src="<%= @creator.img %>" height='50px' width='50px' style='border-radius:100px'>
        <%= @creator.first_name %>
        <%= @creator.last_name %>
        <% end %>
    </div>
  </div>
  <div class="col-md-10 col-md-offset-1">
    <h5>Members</h5>
    <% @users.each do |user| %>
        <% if user != @creator %>
          <div class="col-xs-6 col-md-3 objectives">
            <img src="<%= user.img %>" height='50px' width='50px' style='border-radius:100px'>
            <%= link_to user_path(user) do %>
            <%= user.first_name %>
            <%= user.last_name %>
            <% end %>
        </div>
        <% end %>
    <% end %>
    <div>
</div>
